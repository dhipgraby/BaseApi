{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      },
      {
        "src": "swagger-ui/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/server.ts"
      },
      {
        "src": "/(.*)",
        "dest": "swagger-ui/$1"
      }
    ]
  }
  