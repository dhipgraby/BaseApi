  {
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": [
      "src/**/*",
      "test/**/*",
    ],
    "exclude": ["node_modules", "**/*.spec.ts"]
  }
  